<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.text_field :title %>
  <%= f.text_field :artist_name %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <% (1..3).each do |n|%>
    <input name="song[note_contents][]" id="song_notes_<%=n%>">
  <%end%>
  <br />

  <%= f.submit %>
<% end %>


